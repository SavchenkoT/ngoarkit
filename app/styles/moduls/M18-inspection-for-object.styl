/*!******************************
*
*   M18-inspection-for-object.styl
*
******************************/
.inspection-for-object
  display block
  vertical-align middle
  p
    display inline-block
    font-size 24px
  .btn-group
    background #b7c19f
    &__pull-center
      width 387px
      padding 10px 18px
      margin auto

  .btn-delete
    background-color #fff
    border 1px solid #707071
    color #404041
  .btn-default
    background-color transparent
    &__white
      background-color #fff
      .select
        width 133px
    &__transpar
      border-color 1px solid #6d6e70
      color #6d6e70
    &__save
      border-color 1px solid #fff
      color #fff
      &:hover
        background-color #26a9e0
        color #fff
    &__delete
      background-color #fff
      color #ec1c24
      border-color 1px solid #fff
    &__clean
      background #ff3366
      color #fff
      border-color 1px solid #ff3366
