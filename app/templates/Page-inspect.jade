extends layouts/default
block vars
  - page.title = 'Page Inspect'
  - page.bodyid = 'authpage'
block content
  .container
    .row
      include modules/M01-navbar
      #accordion5.panel-group
          .panel.panel-heading
            btn.btn-link.addcollection
              i.fa.fa-plus(aria-hidden='true', type='text', data-toggle='tooltip', data-placement='top', title='add new panel')
            ul.list-inspection
              li  ОСББ Менеджмент
              li  >
              li  Management
            form.navbar-form.form-horizontal.pull-right(role='search')
              .input-group
                button.btn(type='submit')
                  i.fa.fa-search(aria-hidden='true', type='text', data-toggle='tooltip', data-placement='top', title='Search')
                input.search-box.hidden-xs(type='text', placeholder='Search')
                span.clickable(data-toggle='collapse', data-target='#collapse5', data-parent='#accordion5')
                  i.fa.fa-chevron-down(aria-hidden='true')
              .btn-group.btn-group__elips
                i.fa.fa-ellipsis-v(aria-hidden='true')
      #collapse5.panel-body.not-padd
        include modules/M06-Slavutich
        .container
          .row
            .col-md-3.visible-md-block.visible-lg-block
              include modules/M13-panel-filters
            .col-xs-12.col-md-9
              include modules/M23-object-list
              
              include modules/M25-feature-object-list
