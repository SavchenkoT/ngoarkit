    .container
      .row
        .col-xs-12
          .pull-left
            .header-logo.pull-left
              a(href='#')
                img(src='css/images/Inspection_4.png')
            .header-top-phone.pull-left
              i.fa.fa-phone(aria-hidden='true')
              span +38 044 000 0000
