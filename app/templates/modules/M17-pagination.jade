.example
  .nav
    ul.pagination
      li.disabled
        a(href='#', aria-label='Previous')
          span(aria-hidden='true')
            i.fa.fa-chevron-left
      li.active
        a(href='#') 1
      li
        a(href='#') 2
      li
        a(href='#') 3
      li
        a(href='#') 4
      li
        a(href='#') 5
      li
        a(href='#', aria-label='Next')
          span(aria-hidden='true')
            i.fa.fa-chevron-right
