#catalog
    .catalog_page-nav
        ul.catalog_items
            li.catalog_item
                a.active(href='#') Objects
            li.catalog_item
                a(href='#') Structure
            li.catalog_item
                a(href='#') Object Model
    .catalog_title Objects
    #accordion
        .panel-group
            .panel.panel-heading.panel-heading__inspect.clearfix
                .btn-group
                ul.catalog_buttons-checkbox.pull-left
                    li
                        span.sort-text Sort by :
                        .wrapper-select
                            span.wrapper-select_default date
                                ul.wrapper-select_list
                                    li.wrapper-select_item
                                        | price
                                    li.wrapper-select_item
                                        | date
                                    li.wrapper-select_item
                                        | price
                    li
                        span.sort-text First:
                        .wrapper-select
                            span.wrapper-select_default Critical
                                ul.wrapper-select_list
                                    li.wrapper-select_item
                                        | Critical
                                    li.wrapper-select_item
                                        | Critical
                                    li.wrapper-select_item
                                        | Critical
                ul.catalog_buttons-checkbox.pull-right
                    li
                        input#ok1(type='checkbox')
                        label(for='ok1') Ок
                    li
                        input#attention1(type='checkbox')
                        label(for='attention1') Внимание
                    li
                        input#critically1(type='checkbox')
                        label(for='critically1') Критично
                    li
                        input#uncorrect1(type='checkbox')
                        label(for='uncorrect1') Не корректно
                    li
                        input#unvalued1(type='checkbox')
                        label(for='unvalued1') Без оценки
