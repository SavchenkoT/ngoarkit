head
  //- Windows
  meta(http-equiv='X-UA-Compatible' content='IE=edge')

  //- favicons
  link(rel='apple-touch-icon-precomposed', sizes='144x144', href='css/images/touch/apple-touch-icon-144x144-precomposed.png')
  link(rel='apple-touch-icon-precomposed', sizes='114x114', href='css/images/touch/apple-touch-icon-114x114-precomposed.png')
  link(rel='apple-touch-icon-precomposed', sizes='72x72', href='css/images/touch/apple-touch-icon-72x72-precomposed.png')
  link(rel='apple-touch-icon-precomposed', href='css/images/touch/apple-touch-icon-57x57-precomposed.png')
  link(rel='shortcut icon', href='css/images/touch/favicon.ico')

  meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
  meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, shrink-to-fit=no')
  meta(name='apple-mobile-web-app-capable', content='yes')
  meta(name='apple-mobile-web-app-status-bar-style', content='default')
  meta(name='apple-mobile-web-app-title', content=page.title)

  //- Windows Phone
  <!--[if IE]>
  meta(name='imagetoolbar' content='no')
  meta(name='msthemecompatible' content='no')
  <![endif]-->

  //- Blackberry and etc
  meta(name='cleartype' content='on')
  meta(name='HandheldFriendly' content='True')

  //- No parse telephone and address
  meta(name='format-detection' content='telephone=no')
  meta(name='format-detection' content='address=no')

  block head

  //- SEO
  title= page.title
  meta(name='description' content=page.description)
  meta(name='keywords' content=page.keywords)
  meta(name='copyright' content=page.copyright)
  meta(name='reply-to' content=page.replyTo)
  meta(property="og:site_name" content=page.description)
  meta(property="og:type" content="website" )
  meta(property="og:image" content="css/images/og_logo.jpg" )
  meta(property="og:title" content=page.title)

  //- Stylesheet
  link(href=startingSlash + 'css/main.css' rel='stylesheet')
  link(href='https://fonts.googleapis.com/css?family=Quattrocento+Sans:400,400italic,700,700italic' rel='stylesheet' type='text/css')

  include scripts


